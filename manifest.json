{
  "name": "Time calculator",
  "description": "Calculate time intervals",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["activeTab", "scripting"],
  "web_accessible_resources": [
    {"resources": ["src/index.mjs", "src/Interval.mjs", "src/IntervalParser.mjs", "src/App.mjs"], "matches": ["<all_urls>"]}
  ],
  "content_scripts": [
    {
      "js": [
        "src/script.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}
